def TwoDoors():
  print()
  print("After looking around you see two doors...")
  print()
  time.sleep(x)
  door = ["Door 1","Door 2"]
  user = ""
  while user not in door:
    print("Interact: Door 1/ Door 2")
    print()
    user = input()
    if user == "Door 1":
        Door1()
    elif user == "Door 2":
        Door2() 
        break 
    else:
        print("Please enter a valid option.")
  time.sleep(y)
  print()


def Door1():
  print()
  print("The door leads to a bathroom...")
  print()
  time.sleep(x)
  print("You take a look around...")
  time.sleep(y)
  print()
  senCR1 = "ğ„ğ°... ğˆğ­ ğ¬ğ¦ğğ¥ğ¥ğ¬ ğ¥ğ¢ğ¤ğ ğ«ğ¨ğ­ğ­ğğ§ ğğ ğ ğ¬ ğ¢ğ§ ğ¡ğğ«ğ..."
  for c in senCR1:
       sys.stdout.write(c)
       sys.stdout.flush()
       time.sleep(y)
  time.sleep(0.5)
  print()
  print()
  senCR2 = "ğğ‹ğ„ğ†ğ‚ğ‡... ğ“ğ¡ğğ«ğ'ğ¬ ğ§ğ¨ğ­ğ¡ğ¢ğ§ğ  ğ¡ğğ«ğ ğšğ§ğ²ğ°ğšğ²ğ¬..."
  for c in senCR2:
       sys.stdout.write(c)
       sys.stdout.flush()
       time.sleep(y)
  time.sleep(0.5)
  print()
  TwoDoors()

def Door2():
  print()
  print("The door leads to a dark hallway...")
  time.sleep(y)
  print()
  senDR1 = "ğˆ ğœğšğ§'ğ­ ğ¬ğğ ğšğ§ğ²ğ­ğ¡ğ¢ğ§ğ  ğšğ­ ğšğ¥ğ¥..."
  for c in senDR1:
       sys.stdout.write(c)
       sys.stdout.flush()
       time.sleep(y)
  time.sleep(0.5)
  print()
  print()
  print("Will you go forward or backward?")
  print()
  time.sleep(y)
  print()
  direction = ["Forward","Backward"]
  user = ""
  while user not in direction:
    print("Interact: Forward/Backward")
    print()
    user = input()
    if user == "Forward":
        Hallway1()
        break
    elif user == "Backward":
        TwoDoors()   
    else:
        print("Please enter a valid option.")
  time.sleep(y)
  print()


