  user = ""
  while user not in drawer:
    print("Interact: Drawer 1/Drawer 2/Drawer 3")
    print()
    user = input()
    if user == "Drawer 1":
        Letter2()
    elif user == "Drawer 2":
        Flashlight()
    elif user == "Drawer 3":
        PictureDr()
        break
    else:
        print("Please enter a valid option.")
  time.sleep(y)
  print()
  print()
  print()

